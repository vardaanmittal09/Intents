<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.intents.MainActivity">


    <Button
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="Add"
        android:id="@+id/button"
        android:layout_below="@+id/et_main"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="146dp" />

    <EditText

        android:id="@+id/et_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/et_main2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/et_main"
        android:layout_alignRight="@+id/et_main"
        android:layout_below="@+id/et_main"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="number"
        android:text="" />

    <Button
        android:id="@+id/btnbrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="GoToBrowser"
        android:layout_below="@+id/button"
        android:layout_alignLeft="@+id/button"
        android:layout_alignStart="@+id/button"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button" />


</RelativeLayout>
